		"postcss": "^8.4.21",
		"prettier": "^2.8.4",
		"prettier-plugin-svelte": "^2.9.0",
		"prisma": "^4.11.0",
		"svelte": "^3.56.0",
		"svelte-check": "^3.1.2",
		"svelte-french-toast": "^1.0.3",
		"tailwindcss": "^3.2.7",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.1.4",
		"vitest": "^0.25.8"
	},
	"type": "module",
	"dependencies": {
		"@prisma/client": "^4.11.0",
